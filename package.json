{
  "name": "@taito/react-sheltr",
  "version": "0.0.0-development",
  "description": "Shared element transition helper for React",
  "main": "lib/react-sheltr.js",
  "module": "lib/react-sheltr.es.js",
  "directories": {
    "example": "examples"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "commit": "git-cz",
    "test": "jest",
    "prebuild": "rimraf lib",
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "semantic-release": "semantic-release",
    "clean:install": "rimraf node_modules && npm install",
    "link:examples": "npm link && cd examples && npm link @taito/react-sheltr",
    "travis-deploy-once": "travis-deploy-once"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/TaitoUnited/react-sheltr.git"
  },
  "keywords": [
    "react",
    "shared",
    "element",
    "transition",
    "animation"
  ],
  "files": [
    "lib"
  ],
  "author": "Taito United Oy <support@taitounited.fi>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TaitoUnited/react-sheltr/issues"
  },
  "homepage": "https://github.com/TaitoUnited/react-sheltr#readme",
  "unpkg": "lib/react-sheltr.js",
  "peerDependencies": {
    "react": ">=16.3.0"
  },
  "dependencies": {
    "prop-types": "^15.6.1"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "commitizen": "2.10.1",
    "cz-conventional-changelog": "2.1.0",
    "dom-testing-library": "2.3.2",
    "husky": "0.14.3",
    "jest": "23.0.0",
    "prettier": "^1.10.2",
    "react": "16.4.0",
    "react-dom": "16.4.0",
    "react-testing-library": "3.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.59.3",
    "rollup-plugin-babel": "3.0.4",
    "semantic-release": "^15.5.2",
    "travis-deploy-once": "^5.0.0"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  }
}
